{
  "name": "mnr-request",
  "version": "0.0.0",
  "description": "Opinionated wrapper around node-fetch",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/semistandard --verbose | ./node_modules/.bin/snazzy && ./node_modules/.bin/tape 'test/unit/**/*.test.js' | ./node_modules/.bin/tap-summary",
    "test:integration": "./node_modules/.bin/tape 'test/integration/**/*.test.js' | ./node_modules/.bin/tap-summary",
    "test:dev": "tape 'test/unit/**/*.test.js' | tap-summary"
  },
  "author": {
    "name": "Alexey Gorokhov",
    "email": "alexey.gorokhov@icloud.com",
    "url": ""
  },
  "license": "MIT",
  "devDependencies": {
    "proxyquire": "^2.1.0",
    "semistandard": "^13.0.1",
    "sinon": "^7.3.1",
    "snazzy": "^8.0.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "node-fetch": "^2.6.0",
    "qs": "^6.7.0",
    "stampit": "^4.0.2",
    "verror": "^1.10.0"
  },
  "keywords": [
    "request",
    "node-fetch"
  ],
  "homepage": "https://github.com/AlexeyGorokhov/mnr-request",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexeyGorokhov/mnr-request"
  },
  "bugs": {
    "url": "https://github.com/AlexeyGorokhov/mnr-request/issues"
  }
}
